@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
         url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
         url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}

 .iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-sousuo:before {
  content: "\e612";
} 
.navbar {
  width: 100%;
  background: #fff;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
}

.navbar .navBarWrap {
  width: 85%;
  overflow: hidden;
  display: flex;
  float: left;
}

.navbar .back {
  width: 15%;
  float: right;
  text-align: center;
}
.navbar .back .iconfont{
  display: block;
  margin-top:20rpx;
  color:#7bc267;
}

.navbar-item {
  flex: 1;
  text-align: center;
  font-size: 28rpx;
  color: #7bc267;
}

.navbar-text {
  display: block;
  white-space: nowrap;
  padding: 20rpx 16rpx;
  margin: auto;
}

.navbar-text.active {
  border-bottom: 4rpx solid #7bc267;
  color: #7bc267;
}