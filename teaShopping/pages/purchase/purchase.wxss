/* pages/purchase/purchase.wxss */
.container{
  height: auto;
  overflow: hidden;
}
.content{
  width: 90%;
  height: auto;
  background: #fff;
  margin: 20rpx auto;
}
.content .imgwrap{
  width: 80%;
  height: 600rpx; 
  margin: 0 auto;
}
.content .imgwrap image{
  width: 100%;
  height: 100%;
  vertical-align: top;
  margin-top: 15rpx;
}
.content .logos{
  padding: 100rpx;
  overflow: hidden;
  display: flex;
  flex: 1
}
.content .logos image{
  width:100rpx;
  height: 100rpx;
}
.content .logos .sizes{
  font-size:24rpx;
  margin:18rpx 0 0 30rpx;
}
.btn{
  width:100%;
  height: 100rpx;
  /* border: 1rpx solid red; */
  text-align: center;
  /* margin: auto; */
  line-height: 100rpx;
  font-size: 24rpx;
  color: #fff;
  /* margin-top: 100rpx; */
  /* border-radius:10rpx; */
  position: fixed;
  bottom:0;
  left:0;
  background:#7bc267;
}