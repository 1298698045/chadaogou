  top: 83rpx;
  left: 0;
  border-top: 1rpx solid #dddee3;
}

.sw {
  height: 50rpx;
  overflow: hidden;
  background: #fff;
  border-bottom: 1rpx solid #dddee3;
  display: flex;
  flex-direction: row;
  padding: 26rpx 0;
  margin-top: 1rpx;
}

.sw .item {
  height: 100%;
  flex: 1;
  text-align: center;
  font-size: 28rpx;
  line-height: 50rpx;
  color: #666;
}

.reds {
  color: #7bc267 !important;
}

.sw .item .black {
  color: #666;
}

.sw .item .icons {
  height: 14rpx;
  width: 20rpx;
  display: inline-block;
  margin-top: -28rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 20rpx auto;
  margin-left: 95rpx;
  vertical-align: top;
  transition: all 0.2s ease-out;
}

.sw .item .icon3 {
  height: 14rpx;
  width: 20rpx;
  display: inline-block;
  margin-top: -28rpx;
